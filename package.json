{
    "name": "gh-gql",
    "version": "0.3.1",
    "description": "GitHub GraphQL API client for JavaScript",
    "homepage": "https://github.com/keindev/gh-gql#readme",
    "license": "MIT",
    "author": {
        "name": "Daniil Ryazanov",
        "email": "kein@tagproject.ru"
    },
    "keywords": [
        "github",
        "api",
        "graphql",
        "client"
    ],
    "main": "lib/provider.js",
    "unpkg": "lib/provider.js",
    "module": "lib/provider.js",
    "types": "lib/provider.d.ts",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/keindev/gh-gql.git"
    },
    "bugs": {
        "url": "https://github.com/keindev/gh-gql/issues"
    },
    "scripts": {
        "clean": "rimraf lib coverage",
        "lint": "eslint src/**/*.ts --fix",
        "spellcheck": "cspell -c .vscode/cspell.json --no-summary \"*.*\" \"docs/**/*\" \"**/*.ts\" \"**/*.js\"",
        "test": "jest",
        "changelog": "changelog generate",
        "build:ts": "tsc",
        "build": "npm-run-all clean spellcheck lint test build:*"
    },
    "engines": {
        "node": ">=12.0.0"
    },
    "dependencies": {
        "dotenv": "^8.2.0",
        "graphql-request": "^1.8.2",
        "type-fest": "^0.8.1",
        "universal-user-agent": "^4.0.0"
    },
    "devDependencies": {
        "@types/jest": "^24.0.24",
        "@types/node": "^12.12.21",
        "@typescript-eslint/eslint-plugin": "^2.12.0",
        "@typescript-eslint/parser": "^2.12.0",
        "changelog-guru": "^1.0.0",
        "cspell": "^4.0.41",
        "eslint": "^6.8.0",
        "eslint-config-airbnb-base": "^14.0.0",
        "eslint-config-prettier": "^6.7.0",
        "eslint-plugin-import": "^2.19.1",
        "eslint-plugin-jest": "^23.3.0",
        "husky": "^3.1.0",
        "jest": "^24.9.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^1.19.1",
        "ts-jest": "^24.2.0",
        "typescript": "^3.7.4"
    }
}
