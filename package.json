{
  "name": "gh-gql",
  "version": "1.3.2",
  "description": "GitHub GraphQL API helper for TypeScript",
  "homepage": "https://github.com/keindev/gh-gql#readme",
  "license": "MIT",
  "author": {
    "name": "Daniil Ryazanov",
    "email": "kein@tagproject.ru"
  },
  "keywords": [
    "github",
    "api",
    "graphql",
    "helper",
    "client"
  ],
  "main": "lib/index.js",
  "unpkg": "lib/index.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/keindev/gh-gql.git"
  },
  "bugs": {
    "url": "https://github.com/keindev/gh-gql/issues"
  },
  "scripts": {
    "prepare:clean": "rimraf lib coverage docs src/__generated__",
    "check:lint": "eslint src/**/*.ts",
    "check:spell": "cspell -c .vscode/cspell.json --no-summary \"README.md\" \"src/**/*.ts\"",
    "check:test": "jest",
    "test": "npm-run-all check:*",
    "generate:gql": "dotenv -e .env gql-gen --config codegen.yml",
    "generate:docs": "typedoc",
    "changelog": "changelog generate",
    "generate:ghinfo": "ghinfo generate -d media -t utils",
    "build:ts": "tsc",
    "build": "npm-run-all prepare:* generate:* check:* build:*",
    "util:commit-msg": "changelog lint --message HUSKY_GIT_PARAMS --lowercase-only",
    "util:pre-commit": "npm-run-all check:*"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "graphql": "^15.5.0",
    "graphql-request": "^3.4.0",
    "graphql-tag": "^2.11.0",
    "universal-user-agent": "^6.0.0"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^2.0.2",
    "@graphql-codegen/cli": "^1.21.1",
    "@graphql-codegen/import-types-preset": "^1.18.1",
    "@graphql-codegen/near-operation-file-preset": "^1.17.13",
    "@graphql-codegen/typescript": "1.21.0",
    "@graphql-codegen/typescript-graphql-request": "^3.0.2",
    "@graphql-codegen/typescript-operations": "1.17.15",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.31",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "changelog-guru": "^1.0.0",
    "cspell": "^5.3.7",
    "dotenv-cli": "^4.0.0",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.7",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-optimize-regex": "^1.2.0",
    "eslint-plugin-promise": "^4.3.1",
    "ghinfo": "^1.0.6",
    "husky": "^5.1.3",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.3",
    "typedoc": "^0.20.30",
    "typedoc-plugin-markdown": "^3.6.0",
    "typescript": "^4.2.3"
  }
}
