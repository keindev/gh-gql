{
    "name": "gh-gql",
    "version": "1.0.0",
    "description": "GitHub GraphQL API client for JavaScript",
    "homepage": "https://github.com/keindev/gh-gql#readme",
    "license": "MIT",
    "author": {
        "name": "Daniil Ryazanov",
        "email": "kein@tagproject.ru"
    },
    "keywords": [
        "github",
        "api",
        "graphql",
        "client"
    ],
    "main": "lib/provider.js",
    "unpkg": "lib/provider.js",
    "module": "lib/provider.js",
    "types": "lib/provider.d.ts",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/keindev/gh-gql.git"
    },
    "bugs": {
        "url": "https://github.com/keindev/gh-gql/issues"
    },
    "scripts": {
        "prepare:clean": "rimraf lib coverage src/__generated__",
        "check:lint": "eslint src/**/*.ts --fix",
        "check:spell": "cspell -c .vscode/cspell.json --no-summary \"*.*\" \"docs/**/*\" \"**/*.ts\" \"**/*.js\"",
        "check:test": "jest",
        "generate:gql": "dotenv -e .env gql-gen --config codegen.yml",
        "generate:docs": "typedoc",
        "build:ts": "tsc",
        "build": "npm-run-all prepare:* generate:* check:* build:*",
        "util:changelog": "changelog generate",
        "util:commit-msg": "changelog lint --message HUSKY_GIT_PARAMS --lowercase-only",
        "util:pre-commit": "npm-run-all check:*"
    },
    "engines": {
        "node": ">=12.0.0"
    },
    "dependencies": {
        "dotenv": "^8.2.0",
        "graphql": "^14.5.8",
        "graphql-request": "^1.8.2",
        "graphql-tag": "^2.10.1",
        "universal-user-agent": "^4.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.8.3",
        "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
        "@babel/plugin-proposal-optional-chaining": "^7.8.3",
        "@graphql-codegen/add": "^1.11.2",
        "@graphql-codegen/cli": "^1.11.2",
        "@graphql-codegen/import-types-preset": "^1.11.2",
        "@graphql-codegen/near-operation-file-preset": "^1.11.2",
        "@graphql-codegen/typescript": "1.11.2",
        "@graphql-codegen/typescript-graphql-request": "^1.11.2",
        "@graphql-codegen/typescript-operations": "1.11.2",
        "@types/jest": "^24.9.0",
        "@types/node": "^13.1.7",
        "@typescript-eslint/eslint-plugin": "^2.16.0",
        "@typescript-eslint/parser": "^2.16.0",
        "babel-jest": "^24.9.0",
        "changelog-guru": "^1.0.0",
        "cspell": "^4.0.44",
        "dotenv-cli": "^3.1.0",
        "eslint": "^6.8.0",
        "eslint-config-airbnb-base": "^14.0.0",
        "eslint-config-prettier": "^6.9.0",
        "eslint-plugin-import": "^2.20.0",
        "eslint-plugin-jest": "^23.6.0",
        "husky": "^4.0.10",
        "jest": "^24.9.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^1.19.1",
        "ts-jest": "^24.3.0",
        "typedoc": "^0.16.5",
        "typedoc-plugin-markdown": "^2.2.16",
        "typescript": "^3.7.4"
    }
}
